{
  "name": "ace-api",
  "version": "0.7.45",
  "description": "ACE API",
  "author": {
    "name": "James Homer",
    "email": "hello@homerjam.es",
    "url": "https://homerjam.es"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/homerjam/ace-api.git",
  "main": "dist/api.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec 'node -r dotenv/config' --watch lib --watch server dev.js",
    "dev:inspect": "nodemon --inspect --exec 'node -r dotenv/config' --watch lib --watch server dev.js",
    "build": "webpack --mode production",
    "release": "npm run build && git add --all && git commit --all --message \"build\" && npm version patch && npm publish && git remote | xargs -L1 git push --all",
    "db:design": "source .env; node design/index.js $DB_URL",
    "db:backup": "bash -c 'source .env; couchbackup --url $DB_URL --db $1 | gzip > tmp/$1.json.gz' -- ",
    "db:restore": "bash -c 'source .env; cat $1 | gunzip | couchrestore --url $DB_URL --db $2' -- ",
    "db:list": "source .env; curl -Ss $DB_URL/_all_dbs | jq -r '.[] | @uri' | grep '^[^_]' | xargs echo | sed 's/ /,/g' | pbcopy; echo 'Database list copied to clipboard'",
    "db:shell": "source .env; COUCH_URL=$DB_URL couchshell",
    "db:upgrade": "source .env; util/upgrade.sh $DB_URL",
    "docs": "gulp docs"
  },
  "apidoc": {
    "name": "ACE API",
    "description": "ACE API Documentation",
    "version": "0.0.0",
    "sampleUrl": "http://localhost:5000"
  },
