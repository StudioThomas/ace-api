{
  "name": "ace-api",
  "version": "0.6.14",
  "description": "ACE API",
  "author": {
    "name": "Studio Thomas",
    "email": "hello@studiothomas.co.uk",
    "url": "http://studiothomas.co.uk"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/studiothomas/ace-api.git",
  "main": "dist/ace-api.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "webpack --mode production && git add --all && git commit --all --message \"build\" && npm version patch && npm publish && git remote | xargs -L1 git push --all",
    "mc:section": "babel mjml/components/mc-section/src/index.js -o mjml/components/mc-section/lib/index.js",
    "mc:image": "babel mjml/components/mc-image/src/index.js -o mjml/components/mc-image/lib/index.js",
    "db:design": "source .env; node design/index.js $DB_URL",
    "db:backup": "bash -c 'source .env; couchbackup --url $DB_URL --db $1 | gzip > tmp/$1.json.gz' -- ",
    "db:restore": "bash -c 'source .env; cat $1 | gunzip | couchrestore --url $DB_URL --db $2' -- ",
    "db:list": "source .env; curl -Ss $DB_URL/_all_dbs | jq -r '.[] | @uri' | grep '^[^_]' | xargs echo | sed 's/ /,/g' | pbcopy; echo 'Database list copied to clipboard'",
    "db:shell": "source .env; COUCH_URL=$DB_URL couchshell",
    "db:upgrade": "source .env; util/upgrade.sh $DB_URL"
  },
  "dependencies": {
    "@cloudant/cloudant": "^2.0.2",
    "aws-sdk": "^2.204.0",
    "axios": "^0.18.0",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.1",
    "colors": "^1.1.2",
    "createsend-node": "^0.8.2",
    "deep-diff": "^0.3.8",
    "ejs": "^2.5.7",
    "email-templates": "^3.5.0",
    "embedly": "^2.1.0",
    "fuse.js": "^3.2.0",
    "googleapis": "^26.0.1",
    "hashids": "^1.1.4",
    "html-to-text": "^3.3.0",
    "i18n-iso-countries": "^3.6.1",
    "inky": "^1.3.7",
    "jade": "^1.11.0",
    "json-query": "^2.2.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "measured": "^1.1.0",
    "mime": "^2.2.0",
    "mjml": "^3.3.5",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "nodemailer": "^4.6.0",
    "nodemailer-mailgun-transport": "^1.3.6",
    "password-hash": "^1.2.2",
    "pug": "^2.0.0-rc.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "shippo": "^1.3.1",
    "stripe": "^5.4.0",
    "uuid": "^3.1.0",
    "zencoder": "^2.0.1"
  },
  "devDependencies": {
    "@cloudant/couchbackup": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-node-externals": "^1.6.0"
  }
}
